{
    "extends": [ "plugin:security/recommended", "airbnb-base"],
    "plugins": [
      "security"
    ],
    "env" : {
      "node" : true
    },
    "rules" : {
      "indent": ["error", 4],
      "linebreak-style": ["error", "unix"],
      "quotes": ["error", "double"],
      "semi": ["error", "always"],
      "no-console": "off",
      "consistent-return" : "off"
    },
    "globals": {
      "SequelizeInstance": true,
      "log": true
    }
}
